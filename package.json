{
  "name": "x-ray-ecs-fargate-cdk-typescript-example",
  "version": "1.0.0",
  "description": "Example showing how to set up xray with ecs fargate project",
  "author": {
    "email": "timurabduljalil@gmail.com",
    "name": "Timur Abduljalil"
  },
  "license": "MIT",
  "bin": {
    "template": "infrastructure/ap.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "tsc --build --clean",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint": "eslint . --ext .ts",
    "synth": "cdk synth > template.yml",
    "build::service": "cd service && tsc && docker build -t x-ray-ecs-fargate-cdk-typescript-example-image .",
    "run::service": "docker run -p 49160:8080 n0253365/x-ray-ecs-fargate-cdk-typescript"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.57.0",
    "@types/express": "^4.17.7",
    "@types/jest": "^25.2.1",
    "@types/node": "10.17.5",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^25.5.0",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.1.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-ecr": "1.57.0",
    "@aws-cdk/aws-ecs": "1.57.0",
    "@aws-cdk/aws-ecs-patterns": "^1.57.0",
    "@aws-cdk/aws-iam": "^1.57.0",
    "@aws-cdk/aws-lambda": "^1.57.0",
    "@aws-cdk/aws-lambda-event-sources": "^1.57.0",
    "@aws-cdk/aws-sns": "^1.57.0",
